{
  "name": "@redwoodjs/starter",
  "version": "1.0.0",
  "description": "A bare-bones RedwoodSDK starter",
  "main": "index.js",
  "type": "module",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "vite build",
    "dev": "vite dev",
    "dev:init": "rw-scripts dev-init",
    "preview": "vite preview",
    "worker:run": "rw-scripts worker-run",
    "clean": "npm run clean:vite",
    "clean:vite": "rm -rf ./node_modules/.vite",
    "release": "rw-scripts ensure-deploy-env && npm run clean && npm run build && wrangler deploy",
    "generate": "rw-scripts ensure-env && wrangler types --include-runtime false",
    "check": "npm run generate && npm run types",
    "types": "tsc"
  },
  "dependencies": {
    "force": "^0.0.3",
    "react": "19.3.0-canary-fd524fe0-20251121",
    "react-dom": "19.3.0-canary-fd524fe0-20251121",
    "react-server-dom-webpack": "19.3.0-canary-fd524fe0-20251121",
    "rwsdk": "1.0.0-beta.35"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "1.15.3",
    "@cloudflare/workers-types": "4.20251128.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "~24.5.2",
    "@types/react": "19.1.2",
    "@types/react-dom": "19.1.2",
    "autoprefixer": "^10.4.22",
    "better-sqlite3": "^12.5.0",
    "drizzle-kit": "^0.31.7",
    "drizzle-orm": "^0.44.7",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "typescript": "5.8.3",
    "vite": "7.2.4",
    "wrangler": "^4.51.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "sharp",
      "workerd"
    ]
  }
}
